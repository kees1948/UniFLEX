        opt     nol        lib     sysdef        lib     syserrors        opt     lis** program that would read a FLEX SS/SD disk and drop* the contents in a file*flp2fil equ     *        sys     open,flpdev,0   read        lbes    errext          error, bail out        std     flpfdn*        sys     create,filnam,$3f   write        lbes    errext        std     filfdn*        ldd     flpfdn          init disk        sys     stty,mytty        lbes    errext***loop    ldd     flpfdn          read sector        sys     ind,myseek      select track/sector        lbes    errext        ldd     flpfdn        sys     read,buffer,256 FLEX sector size        lbes    errext        ldd     filfdn          write out        sys     write,buffer,256        lbes    errext        lda     sector        inca        sta     sector        cmpa    #15        bls     loop** one track done*        clra        inca        sta     sector        lda     track        inca        sta     track        cmpa    #77             max track#        blo     loop** done all*        ldd     filfdn        sys     close        ldd     flpfdn        sys     close        ldd     #0        sys     term** errors?*errext  ldd     #2              stderr        sys     write,ermnsg,ermsln        ldd     #1        sys     termmyseek  fcb     seek    for indirect call        fdb     0       seek high offsettrack   rzb     1       seek low offset start tracksector  fcb     1       seek low offset start sector        fdb     0       seek whencemytty   fcb     0       side 0        fcb     0       single dens        fcb     0,0,0,0flpfdn  rzb     2filfdn  rzb     2filnam  fcc     "flexdsksssd",0flpdev  fcc     "/dev/fdc0",0ermnsg  fcc     "***some error occurred, \n",0ermsln  equ     *-ermnsgbuffer  rmb     256        end     flp2fil