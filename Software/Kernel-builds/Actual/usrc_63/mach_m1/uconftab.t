        sttl   Kernel Config Table        pag        name    configtab       global    contab        global    strm,stsk,siob,schl,sfdn,stxt,smnt        global    stim,slok,srtd,sppd,sswd,stlm,sfil,scon,sconbr,smxj        global    stmz,sdlt,sphz,stup,smxm,SWTPCvii,DATsense,ROMmap,NMIhan,SWI2han,MAXMAP        global   BLKHOL,WHTHOL,DSKTRM        data* root device settings major/minorrootmj  equ     0               HDrootmn  equ     0* pipe device settings major/minorpipemj  equ     0               HDpipemn  equ     0* swap device settings major/minorswapmj  equ     0               FLswapmn  equ     0               fd1* important to export these GLOBAL symbols* kernel configuration table*contab  equ     *strm    fcb     12              number of terminals in KERNEL IO spacestsk    fcb     48              number of taskssiob    fcb     64              number of io buffersschl    fcb     64              number of character listssfdn    fcb     64              number of fdn entriesstxt    fcb     20              number of text entriessmnt    fcb     5               number of mountsstim    fcb     24              number of timeouts allowedslok    fcb     32              number of lock table entriessrtd    fcb     rootmj,rootmn   root device numbersppd    fcb     pipemj,pipemn   pipe device numbersswd    fcb     swapmj,swapmn   swap device numberstlm    fcb     0               time limit in 25.6 X seconds (0 disables)sfil    fdb     500             max file size for reg userscon    fdb     BASACI          console terminal address (acia)sconbr  fcb     0               Baud Rate value for system consolesmxj    fcb     MAXJOB          max user task countstmz    fdb     300             time zone (minutes west of gmt)sdlt    fcb     1               daylight savings here (0 if not)sphz    fcb     0               non-zero if power line is 50HZstup    fcb     0               auto update time (0 disables) - value*4smxm    fcb     NBLKS           maximum mem segments per taskFD_SRT  fcb     0               Floppy disk seek rateSWTPCvii fcb    0               non SWTPC vii systemDATsense fcb    $00             CPU09MM3 true DATROMmap  fdb     $feff           Map registers for $E000-$FFFFNMIhan  fdb     nmihanSWI2han fdb     swi2han* access to pages marked as BLKHOL or WHTHOL are trapped* in user contextBLKHOL  fcb     $FD             reads as $3FWHTHOL  fcb     $FC             reads as $00DSKTRM  equ     BLKHOL          end of list markerMAXMAP  fcb     64              Number of hardware maps (DATBOX)