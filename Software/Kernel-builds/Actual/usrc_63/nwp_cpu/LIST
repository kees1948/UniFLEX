                     *  0000               spicmd    rmb    1         HW control register               0000  spista    equ    spicmd    HW status register  0001               spicon    rmb    1         [W]   HW aux control register  0002                         rmb    2         dummy not used                     * shift register access  0004               hibyta    rmb    1         [R/W] hibyte auto 8 clock pulses for SR  0005               lobyta    rmb    1         [R/W] lobyte auto 8 clock pulses for SR  0006               hibyts    rmb    1         [R/W] hibyte static (no clock)  0007               lobyts    rmb    1         [R/W] lobyte static (no clock)                     *               0080  QFF1      equ    %10000000 [R]                          X               0040  SPI_CS_   equ    %01000000 [R/W] spi chip CS_           1               0020  SPI_RST   equ    %00100000 [R/W] spi chip RST_          1FIO Simulation Structure             09:48:13  Nov 04, 2024   Page   81wzspi, basic spi routines                                          0010  SPI_IEN   equ    %00010000 [R/W] spi chip INT enabled   0               0008  SPI_AUT   equ    %00001000 [R/W] auto advance           0               0004  SPI_SR_   equ    %00000100 [R/W] shift register MR_     1               0002  SPI_HLD   equ    %00000010 [R/W] HOLD                   0               0001  SPI_CR_   equ    %00000001 [W]   HC163 MR_              1               0001  SPI_IRQ   equ    %00000001 [R]   /IRQ bit               1                     *               0003  RSVREG    equ    %00000011 reserved register address                       ED10                         org    oldorg                                          *                     * spinit, init the SPI hardware                     *               ED10  spinit    equ    *  ED10 86   65                 lda    #SPI_CS_+SPI_RST+SPI_SR_+SPI_CR_ set idle  ED12 B7   8080               sta    SPIBASE+spicmd  ED15 39                      rts                                          *                     * setup inital port for SPI access                     *                     * A,B are lost A=rwflag, F=register, X=offset                     * F=corrected register bits [7...3]                     * A= R/W flag 1=write/ 0= read                     * X,Y untouched                     *               ED16  spistr2   equ    *  ED16 CE   8080               ldu    #SPIBASE  SPI base TODO  ED19                         trfr   F,B  ED19 1F                      fcb    $1f  ED1A F9                      fcb    F<<4|B                               endm  ED1B 5D                      tstb             register pointer  ED1C 27   09                 beq    01f       OK, valid  ED1E 34   04                 pshs   b  ED20 C4   03                 andb   #RSVREG   reserved loctation  ED22 5D                      tstb             xxx10xxx  is invalid!  ED23 35   04                 puls   b  ED25 27   1A                 beq    09f       invalid access                     * adjust READ/WRITE bit  ED27 4D            01        tsta             cmd flag  ED28 27   04                 beq    04f  ED2A 1A   01                 sec              write   100  ED2C 20   02                 bra    05f  ED2E 1C   FE       04        clc              read    000                     *  ED30 59            05        rolb             set READ/WRITE bit  [2...0]  ED31 58                      lslb             set variable length data [00]  ED32 58                      lslb                     *                     * here the actual SPI action startsFIO Simulation Structure             09:48:13  Nov 04, 2024   Page   82wzspi, basic spi routines                                                *  ED33                         seti   disable   interrupts  ED33 1A   50                 orcc   #FF|IF                               endm  ED35 12                      nop                     *       lda    #SPI_RST+SPI_SR_+SPI_CR_+SPI_AUT+SPI_IEN low  ED36 86   2D                 lda    #SPI_RST+SPI_SR_+SPI_CR_+SPI_AUT low  ED38 A7   C4                 sta    spicmd,u  ED3A AF   44                 stx    hibyta,u  and shift out  ED3C E7   44                 stb    hibyta,u  and shift out  ED3E 1A   04                 sez  ED40 39                      rts  ED41 1C   FB       09        clz  ED43 39                      rts                                          *                     * X = offset, unchanged                     * F = register#                     * read BYTE from SPI                     * return B=value, D,X,Y,U preserved                     *               ED44  byterd    equ    *  ED44 34   47                 pshs   cc,d,u  ED46 4F                      clra  ED47 8D   CD                 bsr    spistr2 >ED49 1026 007F               lbne   09f  ED4D E6   44                 ldb    hibyta,u  extra clocks for first data                     *  ED4F E6   45                 ldb    lobyta,u  read byte  ED51 4F                      clra  ED52 ED   61                 std    1,s  ED54 20   6A                 bra    sprend                                          *                     * X = offset                     * F = register#                     * read WORD from SPI                     * return D=value,  D,X,Y,U preserved                     *               ED56  wordrd    equ    *  ED56 34   47                 pshs   cc,d,u  ED58 4F                      clra >ED59 BD   ED16               jsr    spistr2  ED5C 26   6E                 bne    09f  ED5E E6   44                 ldb    hibyta,u  extra clocks for first data                     *  ED60 A6   45                 lda    lobyta,u  read byte  ED62 E6   45                 ldb    lobyta,u  read byte  ED64 ED   61                 std    1,s  ED66 20   58                 bra    sprend                                          *FI