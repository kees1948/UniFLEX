                  *  EB68 CC   0004               ldd    #NBRQLN   actual size of request  EB6B 108E 001C               ldy    #fifo_us2 where request is present  EB6F BD   F313               jsr    WRFB2SK  EB72 C6   20                 ldb    #WCSEND   tell socket we put it  EB74 E7   44                 stb    wzcmnd,u  EB76 BD   F00A               jsr    PSRCR                     *FIO Simulation Structure             09:48:11  Nov 04, 2024   Page   75wz socket interface routines                                             * read is 2 phase, receive response from server                     *  EB79 BD   E647     sknbrp2   jsr    skfres    reset important info  EB7C BD   F1F5               jsr    GSRRXRS   check response  EB7F 34   06                 pshs   d  EB81 EC   4A                 ldd    wzrqln,u  requested data  EB83 C3   0004               addd   #NBRQLN  EB86 10A3 E1                 cmpd   0,s++     allows for other data packet sizes  EB89 23   1D                 bls    01f  EB8B BD   F03E               jsr    GSRSR  EB8E E7   45                 stb    wzstat,u  EB90 C1   17                 cmpb   #WSESTB   check socket state  EB92 26   11                 bne    sknbe1  EB94 C1   1C                 cmpb   #WSCLWT  EB96 27   0D                 beq    sknbe1    bail out  EB98 108E EB79               ldy    #sknbrp2  wait until                     *                     * poll loop                     *  EB9C 7E   E7D0     02        jmp    polext                     *  EB9F 108E EB47     sknbrp1   ldy    #sknbrr  EBA3 20   F7                 bra    02b                     *  EBA5 C6   C0       sknbe1    ldb    #E_SOCKET  EBA7 39                      rts                     * we have data in socket  EBA8 C6   82       01        ldb    #R_RQRBLK  EBAA 4F                      clra  EBAB 39                      rts                                          ***************************************************************                     * here data is moved from the socket to the fifo                     * write 4 bytes back to fifo_us2/fifo_us3                     ***************************************************************               EBAC  sknbrd    equ    *  EBAC BD   E647               jsr    skfres  EBAF BD   E63B               jsr    wzdevt  EBB2 1026 FD50     02        lbne   rdrqer1  EBB6 BD   E64E               jsr    skctcp  EBB9 26   F7                 bne    02b                     *                     * when we arrive here, the data has arrived                     *  EBBB EC   4A       01        ldd    wzrqln,u  request sets the buffer size  EBBD 34   06                 pshs   d  EBBF C3   0004               addd   #NBRQLN  EBC2 ED   4A                 std    wzrqln,u  fool RDSK2FB  EBC4 108E 001C               ldy    #fifo_us2 data contains cmd info  EBC8 BD   F2C5               jsr    RDSK2FB  EBCB C6   40                 ldb    #WCRECV   tell socket we took it  EBCD E7   44                 stb    wzcmnd,uFIO Simulation Structure             09:48:11  Nov 04, 2024   Page   76wz socket interface routines                          EBCF BD   F00A               jsr    PSRCR                     * transfer only data part  EBD2 35   06                 puls   d  EBD4 ED   48                 std    wzxfer,u  EBD6 ED   4A                 std    wzrqln,u                     *  EBD8 C6   83                 ldb    #R_RDRBLK tell caller, data in fifo_us2 and fifo  EBDA 39                      rts                                          ***************************************************************                     *                     * netblock, write block                     *                     ***************************************************************               EBDB  sknbsr    equ    *  EBDB BD   E647               jsr    skfres                     *                     * is the DEVICE (still) active                     *  EBDE BD   E63B               jsr    wzdevt  EBE1 1026 FD21     02        lbne   rdrqer1  EBE5 BD   E64E               jsr    skctcp  EBE8 26   F7                 bne    02b                     *  EBEA BD   F03E               jsr    GSRSR     status  EBED E7   45                 stb    wzstat,u  EBEF C1   17                 cmpb   #WSESTB   still there?  EBF1 26   16                 bne    sknbe10                     *  EBF3 BD   F1A9               jsr    GSRTXFR   get free space  EBF6 34   06                 pshs   d  EBF8 CC   0004               ldd    #NBRQLN  EBFB E3   4A                 addd   wzrqln,u  EBFD 10A3 E1                 cmpd   0,s++  EC00 23   0F                 bls    70f                     *  EC02 108E EBDB               ldy    #sknbsr  EC06 7E   E7D0               jmp    polext                     *  EC09 CC   0000     sknbe10   ldd    #0  EC0C ED   48                 std    wzxfer,u  EC0E C6   C0                 ldb    #E_SOCKET  EC10 39                      rts                       EC11 4F            70        clra  EC12 C6   80                 ldb    #R_RQSBLK  EC14 39                      rts                                          ***************************************************************                     *                     *                     ***************************************************************FIO Simulation Structure             09:48:12  Nov 04, 2024   Page   77wz socket interface routines                                       EC15  sknbsd    equ    *  EC15 BD   E647               jsr    skfres  EC18 BD   E63B               jsr    wzdevt  EC1B 1026 FCE7     02        lbne   rdrqer1  EC1F BD   E64E               jsr    skctcp  EC22 26   F7                 bne    02b                                          *                     * data to write, cnd +data                     *  EC24 EC   4A       70        ldd    wzrqln,u  EC26 34   06                 pshs   d  EC28 C3   0004               addd   #NBRQLN  EC2B ED   4A                 std    wzrqln,u  fool WFB2SK  EC2D 108E 001C               ldy    #fifo_us2 where cmd sits  EC31 BD   F313               jsr    WRFB2SK  EC34 C6   20                 ldb    #WCSEND   send it now  EC36 E7   44                 stb    wzcmnd,u  EC38 BD   F00A               jsr    PSRCR  EC3B 35   06                 puls   d  EC3D ED   48                 std    wzxfer,u  make it correct  EC3F ED   4A                 std    wzrqln,u  EC41 4F                      clra  EC42 C6   81                 ldb    #R_SNDBLK tell host we did  EC44 39                      rts                                          ***************************************************************                     *                     * special function, tstval,x is indicator                     * X hold the task table                     * U holds the SPECIAL sock address (NO wz....  access!)                     *               EC45  skspcl    equ    *                     *  EC45 34   10                 pshs   x  EC47 E6   0C                 ldb    tstval,x  get index  EC49 C1   03                 cmpb   #MAXTBL  EC4B 22   18                 bhi    01f       illegal  EC4D 58                      aslb  EC4E 8E   EC6C               ldx    #functb  EC51 10AE 85                 ldy    b,x       get special function  EC54 27   0F                 beq    01f       not valid  EC56 35   10                 puls   x                     *  EC58 AD   A4                 jsr    0,y       do function  EC5A 26   05                 bne    90f                     *  EC5C 4F                      clra  EC5D C6   0F                 ldb    #R_SPCL  EC5F 20   0A                 bra    99f                     *  EC61 86   A2       90        lda    #162FIO Simulation Structure             09:48:12  Nov 04, 2024   Page   78wz socket interface routines                          EC63 20 