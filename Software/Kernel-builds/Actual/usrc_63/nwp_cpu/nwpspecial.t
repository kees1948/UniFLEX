** nwpspecial, routines that communicate a bit differently************************************************************************ memp2ff, mem pointer to fifo* copy 512 bytes addressed by fifo_us3 to fifo** X=taskstruct* U=wzsock address***********************************************************************memp2ff equ     *        ldd     fifo_us3        get address        tfr     b,a        clrb        lsla                    512 byte chunks        tfr     d,x        ldd     fio_fsz        trfr    D,W        ldy     #fifo        tfm1    X,Y        clra        ldb     #R_RDOK        rtsnetboot equ     *        rts